.sb-page-header.pb-10(class=pageHeaderStyle, class=pageHeaderBackground)
    .container-fluid
        .sb-page-header-content.py-5
            h1.sb-page-header-title
                .sb-page-header-icon
                    i(data-feather=pageIcon)
                span #{pageTitle}
            .sb-page-header-subtitle
                | #{pageSummary}
            if breadcrumbs

                ol.breadcrumb.mt-4.mb-0
                    li.breadcrumb-item
                        a(href='index.html') Dashboard
                    li.breadcrumb-item.active #{pageTitle}

            else
